{
  "Rules": [
    {
      "ID": "KeepForever",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "manifests/"
      },
      "Transitions": [
        {
          "Days": 30,
          "StorageClass": "INFREQUENT_ACCESS"
        },
        {
          "Days": 90,
          "StorageClass": "ARCHIVE"
        }
      ],
      "NoncurrentVersionExpiration": {
        "NoncurrentDays": 99999
      },
      "Expiration": {
        "Days": 2555  // 7 years retention for compliance
      }
    }
  ]
}
